{
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "cross-env": "^7.0.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "3.3.2",
    "rimraf": "^5.0.7",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18.20",
    "yarn": ">=1.22"
  },
  "main": "",
  "name": "backend-parking",
  "scripts": {
    "build": "yarn build:validate && yarn build:clean && yarn build:generate",
    "build:clean": "rimraf ./build; exit 0",
    "build:validate": "tsc --noEmit",
    "build:generate": "swc ./src -d build --strip-leading-paths",
    "dev": "tsnd --respawn --inspect=0.0.0.0:9229 -r tsconfig-paths/register ./src/server/app.ts",
    "format": "prettier --write \"./src/**/*.ts\"",
    "lint": "eslint . --ext .ts --quiet --fix --ignore-path .eslintignore",
    "prepare": "husky",
    "start": "node ./server/app.js",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:coverage": "jest --coverage",
    "test:integration:watch": "jest --watch --config jest.integration.config.js --watch",
    "test:integration": "cross-env NODE_ENV=test jest --config jest.integration.config.js"
  },
  "version": "0.1.0",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
