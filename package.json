{
  "name": "backend-parking",
  "version": "0.1.0",
  "main": "",
  "engines": {
    "node": ">=18.20",
    "yarn": ">=1.22"
  },
  "scripts": {
    "start": "node ./build/app.js",
    "dev": "tsnd --respawn --inspect=0.0.0.0:9229 ./src/app.ts",
    "build": "yarn build:validate && yarn build:clean && tsc",
    "build:validate": "tsc --noEmit",
    "build:clean": "rimraf ./build",
    "lint": "eslint . --ext .ts --quiet --fix",
    "format": "prettier --write \"./src/**/*.ts\"",
    "prepare": "husky"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "3.3.2",
    "rimraf": "^5.0.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3"
  }
}
